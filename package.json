{
  "name": "bigpipe",
  "version": "0.9.0",
  "description": "Bigpipe a radical new web framework for Node.js that's inspired by Facebook's bigpipe concept.",
  "main": "index.js",
  "engine": "node >= 0.10.0",
  "scripts": {
    "test": "mocha --reporter spec --ui bdd $(find test -name '*.test.js')",
    "watch": "mocha --watch --reporter spec --ui $(find test -name '*.test.js')",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- --reporter spec --ui bdd $(find test -name '*.test.js')",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- --reporter spec --ui bdd $(find test -name '*.test.js')"
  },
  "pre-commit": [
    "compile",
    "test"
  ],
  "dependencies": {
    "404-pagelet": "1.0.x",
    "500-pagelet": "1.0.x",
    "async": "0.9.x",
    "bigpipe.js": "0.8.x",
    "bootstrap-pagelet": "1.0.x",
    "create-server": "0.0.x",
    "demolish": "1.0.x",
    "diagnostics": "0.0.x",
    "eventemitter3": "0.1.x",
    "fabricator": "0.4.x",
    "fusing": "0.4.x",
    "mkdirp": "0.5.x",
    "smithy": "0.5.x",
    "supply": "0.0.x",
    "temper": "0.2.x",
    "zipline": "1.0.x"
  },
  "devDependencies": {
    "assume": "1.0.x",
    "istanbul": "0.3.x",
    "mocha": "2.1.x",
    "pagelet": "0.8.x",
    "pre-commit": "0.0.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bigpipe/bigpipe.git"
  },
  "bugs": {
    "url": "https://github.com/bigpipe/bigpipe/issues"
  },
  "keywords": [
    "pagelet",
    "pagelets",
    "realtime",
    "real-time",
    "framework",
    "modular",
    "bigpipe"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "homepage": "http://bigpipe.io"
}
